{
  "name": "dtk",
  "version": "1.0.0",
  "description": "Electron + TypeScript + React sample com Atomic Design e Glassmorphism",
  "main": "src/main/index.js",
  "homepage": "./",
  "author": "Eduardo",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "NODE_ENV=development nodemon --watch src/main --ext ts --ignore src/main/preload.js --exec \"electron -r ts-node/register\" .",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "build:preload": "esbuild src/main/preload.ts --bundle --platform=node --target=es2020 --outfile=src/main/preload.js",
    "start": "electron .",
    "package": "npm run build && electron-builder",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\""
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@xterm/xterm": "^5.5.0",
    "codemirror": "^6.0.2",
    "dotenv": "^17.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sql.js": "^1.13.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ssh2": "^1.17.0",
    "sshpk": "^1.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.2.0",
    "vite": "^4.5.0",
    "wait-on": "^7.0.0"
  }
}
